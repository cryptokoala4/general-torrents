<div class="navbar navbar-default">
  <div class="navbar-header">
    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse" aria-expanded="false">
      <span class="sr-only">Toggle navigation</span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
    </button>
    <a class="navbar-brand" href="/"><img id="logo-nav" src="/images/ga.png"></a>
    <form class="form-inline navbar-brand" style="display: inline;" role="search">
      <div class="input-group stylish-input-group">
        <input type="text" class="form-control"  placeholder="Search" >
        <span class="input-group-addon"><button type="submit"><span class="glyphicon glyphicon-search"></span></button></span>

      </div>
<!--  -->
    </form>
  </div>

  <div class="collapse navbar-collapse" id="navbar-collapse">
    <ul class="navbar-right list-inline">
      <li>
        <a class="center-upload" href="torrents/upload"><strong>Upload Torrent  </strong><img class="comment-icon" src="/images/torrent-ul.png"></a>
      </li>
    <% if(user){ %>
      <div class="nav-logged center-upload">
        <% if(user.fb.access_token){ %>
          <img src="/images/login_fb.png"><span><%= "  " + user.fb.firstName + " " + user.fb.lastName %></span>
        <% } else { %>
          <img src="/images/login_gh.png"><span><%= "  " + user.gh.username %></span>
        <% } %>
        <a href="/signout"><strong>Logout</strong><p class="glyphicon glyphicon-log-out"></p></a>
      </div>
    <% } else { %>
      <li>
        <a href="/auth/facebook" class="btn btn-block btn-social btn-facebook">
        <span class="fa fa-facebook"></span> Sign in with Facebook</a>
      </li>
      <li>
        <a href="/auth/github" class="btn btn-block btn-social btn-github">
        <span class="fa fa-github"></span> Sign in with Github</a>
      </li>
    <% } %>
    </ul>
  </div>
</div>
